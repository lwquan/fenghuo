@-webkit-keyframes mouse {
	0% {
	opacity:1
}
80% {
	opacity:0;
	-webkit-transform:translate3d(0,100%,0);
	transform:translate3d(0,100%,0)
}
100% {
	opacity:0;
	-webkit-transform:translate3d(0,-100%,0));
	transform:translate3d(0,-100%,0)
}
}@keyframes mouse {
	0% {
	opacity:1
}
80% {
	opacity:0;
	-webkit-transform:translate3d(0,100%,0);
	transform:translate3d(0,100%,0)
}
100% {
	opacity:0;
	-webkit-transform:translate3d(0,-100%,0));
	transform:translate3d(0,-100%,0)
}
}.mouse {
	-webkit-animation-name:mouse;
	animation-name:mouse
}